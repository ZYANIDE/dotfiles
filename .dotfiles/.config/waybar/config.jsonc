{
  "output": "HDMI-A-2",
  "layer": "top",
  "position": "top",
  "height": 32,
  "spacing": 0,
  "modules-left": [
    "custom/notification",
    "custom/pacman",
    "wlr/taskbar"
  ],
  "modules-center": [
    "clock"
  ],
  "modules-right": [
    "bluetooth",
    "network",
    "pulseaudio",
    "custom/music",
    "backlight",
    "battery",
    "group/power-menu"
  ],
  "bluetooth": {
    "format": "Û∞Ç≤",
    "format-on": "{icon}",
    "format-off": "{icon}", 
    "format-connected":"{icon}",
    "format-icons":{
      "on":"Û∞ÇØ",
      "off": "Û∞Ç≤",
      "connected": "Û∞Ç±"
    },
    "on-click": "blueman-manager",
    "tooltip-format-connected":"{device_enumerate}"
  },
  "network": {
    "format-wifi": "{}",
    "format-ethernet": "Û∞àÄ ",
    "format-disconnected": "Û∞§† ",
    "interval": 5,
    "format-icons": ["Û∞§ü", "Û∞§¢", "Û∞§•", "Û∞§®"],
    "tooltip-format-wifi": "{essid} ({signalStrength}%)",
    "tooltip-format-ethernet": "{ifname} üñß ",
    "on-click": "nm-connection-editor"
  },
  "pulseaudio": {
    "format": "{icon}  {volume}%",
    "format-muted": "Óª®",
    "format-icons": ["ÔÄ¶", "ÔÄß", "ÔÄ® "],
    "on-click": "pavucontrol"
  },
  "custom/music": {
    "format": "Û∞ùö  {}",
    "escape": true,
    "interval": 5,
    "tooltip": false,
    "exec": "playerctl metadata --format='{{ artist }} - {{ title }}' 2> /dev/null",
    "on-click": "playerctl play-pause",
    "max-length": 50
  },
  "backlight": {
    "format": "{icon}  {percent}%",
    "format-icons": ["Óéç","Û∞Éú", "Û∞Éõ", "Û∞Éû","Û∞Éù","Û∞Éü","Û∞É†"],
    "tooltip": false
  },
  "battery": {
    "interval": 2,
    "states": {
      // "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{icon}  {capacity}%",
    "format-full": "{icon}  {capacity}%",
    "format-charging": "Ôá¶ {capacity}%",
    "format-plugged": "Ôá¶ {capacity}%",
    "format-alt": "{icon} {time}",
    // "format-good": "", // An empty format will hide the module
    // "format-full": "",
    "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"],
    "bat-compatibility": true
  },
  "clock": {
    "timezone": "CET",
    "tooltip": false,
    "format": "{:%H:%M  -  %A, %d}",
    "interval": 1
  },
  "wlr/taskbar": {
    "all-outputs": true,
    "icon-size": 24,
    "on-click": "activate"
  },
  "custom/uptime": {
    "format": "{}",
    "format-icon": ["ÔÅ¢"],
    "tooltip": false,
    "interval": 1600,
    "exec": "sh $HOME/.config/waybar/scripts/uptime.sh"
  },
  "cpu": {
    "interval": 1,
    "format": "Ôãõ{usage:>2}%",
    "format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà"],
    "on-click": "alacritty -e htop"
  },
  "memory": {
    "interval": 30,
    "format": "ÓøÖ {used:0.1f}G ({percentage}%)",
    "tooltip-format": "Memory"
  },
  "temperature": {
    "critical-threshold": 80,
    "format": "Ôãâ"
  },
  "group/power-menu": {
    "orientation": "horizontal",
    "drawer": {
      "transition-duration": 600,
      "transition-left-to-right": false,
      "click-to-reveal": false
    },
    "modules": ["custom/power", "custom/reboot", "custom/sleep", "custom/lock"]
  },
  "custom/lock": {
    "format": "ÔÄ£",
    "on-click": "loginctl lock-session",
    "tooltip": true,
    "tooltip-format": "Lock Screen"
  },
  "custom/sleep": {
    "format": "Û∞§Ñ",
    "on-click": "systemctl suspend",
    "tooltip": true,
    "tooltip-format": "Sleep"
  },
  "custom/reboot": {
    "format": "ÔÄ°",
    "on-click": "systemctl reboot",
    "tooltip": true,
    "tooltip-format": "Reboot"
  },
  "custom/power": {
    "format": "ÔÄë",
    "on-click": "systemctl poweroff",
    "tooltip": true,
    "tooltip-format": "Power Off"
  },
  "custom/pacman": {
    "format": "Û∞Ö¢ {}",
    "interval": 30,
    "exec": "pacman -Qu | wc -l",
    "exec-if": "exit 0",
    "on-click": "alacritty -e bash -c 'sudo pacman -Syu; echo Done - Press enter to exit; read'; pkill -SIGRTMIN+8 waybar",
    "signal": 8,
    "tooltip": false
  },
  "custom/notification": {
    "tooltip": false,
    "format": "Óöê",
    "on-click": "swaync-client -t -sw",
    "escape": true
  }
}
